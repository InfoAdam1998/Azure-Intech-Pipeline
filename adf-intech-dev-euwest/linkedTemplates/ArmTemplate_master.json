{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "adf-intech-dev-euwest"
		},
		"ls_akv_intech_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "https://akv-intech.vault.azure.net/"
		},
		"ls_dbw_intech_properties_typeProperties_existingClusterId": {
			"type": "string",
			"defaultValue": "1204-000337-0m27x9de"
		},
		"ls_dl_intech_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://dlintech001.dfs.core.windows.net/"
		},
		"ls_github_intech_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://raw.githubusercontent.com"
		},
		"ls_sql_server_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "ADAMOULIS\\SQLEXPRESS"
		},
		"ls_sql_server_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "AdventureWorks2019"
		},
		"ls_sql_server_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "adam"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "adf-intech-dev-euwest_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"ls_akv_intech_properties_typeProperties_baseUrl": {
						"value": "[parameters('ls_akv_intech_properties_typeProperties_baseUrl')]"
					},
					"ls_dl_intech_properties_typeProperties_url": {
						"value": "[parameters('ls_dl_intech_properties_typeProperties_url')]"
					},
					"ls_github_intech_properties_typeProperties_url": {
						"value": "[parameters('ls_github_intech_properties_typeProperties_url')]"
					},
					"ls_dbw_intech_properties_typeProperties_existingClusterId": {
						"value": "[parameters('ls_dbw_intech_properties_typeProperties_existingClusterId')]"
					},
					"ls_sql_server_properties_typeProperties_server": {
						"value": "[parameters('ls_sql_server_properties_typeProperties_server')]"
					},
					"ls_sql_server_properties_typeProperties_database": {
						"value": "[parameters('ls_sql_server_properties_typeProperties_database')]"
					},
					"ls_sql_server_properties_typeProperties_userName": {
						"value": "[parameters('ls_sql_server_properties_typeProperties_userName')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "adf-intech-dev-euwest_ArmTemplate_1",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_1.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'adf-intech-dev-euwest_ArmTemplate_0')]"
			]
		}
	]
}